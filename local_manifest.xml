<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <!--remotes -->
    <remote name="Orel6505"   fetch="https://github.com/Orel6505" />
    <remote name="Linux4"     fetch="https://github.com/Linux4" />           
    <remote name="LOS"  fetch="https://github.com/LineageOS" />
    <remote name="TheMuppets" fetch="https://github.com/TheMuppets" />
    <remote name="PE-devices" fetch="https://github.com/PixelExperience-Devices" />
    <remote name="PE-blobs"   fetch="https://gitlab.pixelexperience.org/android/vendor-blobs" />
    <remote name="PE"         fetch="https://github.com/PixelExperience" />
    <remote name="vic"        fetch="https://gitHub.com/no-leaks-inside" />

    <!--Device Tree - you can track from LineageOS if you want-->

	<project name="device_samsung_exynos9820" path="device/samsung" remote="vic"  revision="snow" />
	
	<!--Vendor Tree - You can track from TheMuppets if you want-->
    <project name="vendor_samsung"                   path="vendor/samsung"                   remote="PE-blobs"    revision="twelve" clone-depth="1" />
    
    <!--Kernel Tree - You can track from The LineageOS if you want-->
    <project name="kernel_samsung_exynos9820"        path="kernel/samsung/exynos9820"        remote="PE-devices"  revision="twelve" clone-depth="1" />
    
    <!-- Dependenciens -->
    <project name="android_device_samsung_slsi_sepolicy"    path="device/samsung_slsi/sepolicy"      remote="LOS"   revision="lineage-19.1" />
    <project name="hardware_samsung"                path="hardware/samsung"                  remote="PE"   revision="twelve" />
	
</manifest>
